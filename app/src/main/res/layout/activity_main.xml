<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/llllllll"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center_horizontal"
    tools:context="com.trust.demo.trusttool.MainActivity">


    

    
    <com.trust.statusbarlibrary.view.TrustStatusBarView
        android:id="@+id/test_trust_bar"
        android:layout_width="match_parent"
        android:layout_height="50dp" />


    <Button
        android:layout_marginTop="10dp"
        android:id="@+id/ble_btn"
        android:text="蓝牙"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:layout_marginTop="10dp"
        android:id="@+id/live_btn"
        android:text="直播"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />





    <Button
        android:layout_marginTop="10dp"
        android:id="@+id/open_start_btn"
        android:text="调用外部高德地图和百度地图导航"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <ImageView
        android:src="@drawable/grile"
        android:id="@+id/test_img"
        android:layout_width="50dp"
        android:layout_height="50dp" />

    <Button
        android:layout_marginTop="10dp"
        android:id="@+id/view_btn"
        android:text="自定义view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />


</LinearLayout>
